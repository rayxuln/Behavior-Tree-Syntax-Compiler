
import show: 'bt/set_text.gd'
import yield: 'bt/wait_signal.gd'



tree
	sequence
		show msg: 'click the button!' n: 'Label'
		yield obj: 'Button' sig: 'pressed'
		show msg: 'click again!' n: 'Label'
		yield obj: 'Button' sig: 'pressed'
		show msg: 'Thanks!' n: 'Label'
		yield obj: 'Button' sig: 'pressed'



